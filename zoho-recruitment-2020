package kalanidhi;
import java.util.Scanner;

public class zoho {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n,i,j,k;
		Scanner s=new Scanner(System.in);
		n=s.nextInt();
		k=0;
		int l=n;
		int b=n;
		int[][]a=new int[n][n];
		for(i=n;i>0;i--) {
			for(j=n;j>0;j--) {
				
				System.out.print(b+" ");
				if(l<n&&j>i) {
					b--;
				}	
				}
			l--;
			b=n;
			System.out.println();
		}
		

	}

}
